{
  "name": "my-package",
  "version": "1.0.0",
  "description": "rss-school tasks",
  "author": "ssstarss",
  "main": "index.ts",
  "license": "ISC",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --ext .ts --fix",
    "format": "prettier **/*.{ts,css} --write",
    "test": "jest",
    "build": "webpack",
    "serve": "webpack serve --open --mode development",
    "watch": "webpack --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  },
  "targets": {
    "default": {
      "distDir": "./dist",
      "sourceMap": true
    }
  },
  "devDependencies": {
    "@jest/globals": "29.4.3",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "29.4.3",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "style-loader": "^3.3.4",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": ">=4.7.4 <5.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@commercetools/sdk-client": "^3.0.0",
    "@commercetools/sdk-middleware-auth": "^7.0.1",
    "@commercetools/sdk-middleware-http": "^7.0.4",
    "@commercetools/typescript-sdk": "^0.0.22",
    "dotenv": "^16.4.5",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1"
  }
}
